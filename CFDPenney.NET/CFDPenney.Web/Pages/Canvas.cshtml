@page
@model CanvasModel
@{
    ViewData["Title"] = "CFD Penney - Collaborative Annotation Tool";
    Layout = "_CanvasLayout";
}

@section Head {
    @await Html.PartialAsync("Shared/_CanvasHead")
}

@await Html.PartialAsync("Shared/_CanvasHeader")

@await Html.PartialAsync("Shared/_CanvasWorkspace")

@await Html.PartialAsync("Shared/_CollaborationModal")

@await Html.PartialAsync("Shared/_PopupModal")

@await Html.PartialAsync("Shared/_CanvasFooter")

@section Scripts {
    <script type="module" src="~/js/main.js"></script>
}
